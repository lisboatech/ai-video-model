{
  "functions": {
    "app/api/render-video/route.js": {
      "maxDuration": 300
    },
    "app/api/generate-script/route.js": {
      "maxDuration": 60
    },
    "app/api/create-voice-elevenlabs/route.jsx": {
      "maxDuration": 120
    }
  },
  "build": {
    "env": {
      "REMOTION_CLI_TIMEOUT": "300000"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
